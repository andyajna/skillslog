<h3>List of Skill Dates</h3>

<table class="table">
  <tr>
    <th>User</th>
    <th>Domain</th>
    <th>Skill</th>
    <th>Date observed</th>
    <th>Date done</th>
    <th>Date taught</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_histories.each do |user_history| %>
  <tr>
    <td><%= user_history.user_id %></td>
    <td><%= user_history.domain_id %></td>
    <td><%= user_history.skill_id %></td>
    <td><%= user_history.date_observed %></td>
    <td><%= user_history.date_done %></td>
    <td><%= user_history.date_taught %></td>
    <td><%= link_to 'Show', user_history %></td>
    <td><%= link_to 'Edit', edit_user_history_path(user_history) %></td>
    <td><%= link_to 'Destroy', user_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

