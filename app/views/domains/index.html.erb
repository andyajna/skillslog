<h3>List of Domains</h3>
<%= link_to 'Add A New Domain', new_domain_path %>
<p>&nbsp;</p>
<table class="table">
  <tr>
    <th>Name</th>
    <th>Curriculum</th>
    <th>Stream</th>
    <th></th>
    <th></th>
  </tr>

<% @domains.each do |domain| %>
  <tr>
    <td><%= link_to domain.name, domain %></td>
    <td><%= domain.curriculum %></td>
    <td><%= domain.stream %></td>
    <td><%= link_to 'Edit', edit_domain_path(domain) %></td>
    <td><%= link_to 'Destroy', domain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />


